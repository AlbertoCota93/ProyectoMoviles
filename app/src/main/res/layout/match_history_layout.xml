<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_marginTop="@dimen/dota_matchHistory_margin_top">
    <TextView
        android:id="@+id/match_history_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="@dimen/activity_dota_text"
        android:layout_marginLeft="@dimen/profile_margin_left"
        />
    <TextView
        android:id="@+id/match_history_hero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="@dimen/activity_dota_text"
        android:layout_marginLeft="@dimen/profile_margin_left"
        />
     <ImageView
         android:id="@+id/match_history_hero_pic"
         android:layout_width="@dimen/dota_matchHistory_width"
         android:layout_height="@dimen/dota_matchHistory_height"
         android:layout_marginLeft="@dimen/profile_margin_left"/>
    <TextView
        android:id="@+id/match_history_kda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="@dimen/activity_dota_text"
        android:layout_marginLeft="@dimen/profile_margin_left"/>


</LinearLayout>